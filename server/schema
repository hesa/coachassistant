Club
    uuid UUID
    name Text
    created UTCTime default=now()
    Primary uuid
    UniqueClubName name
Member
    uuid UUID
    name Text
    created UTCTime default=now()
    clubUuid UUID
    teamUuid UUID Maybe
    Primary uuid
    Foreign Club clubUuidKey clubUuid
